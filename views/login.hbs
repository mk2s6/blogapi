{{> header }}
	
	<div id="main_body">
		<div class="main_content">
			<h1>{{title}}</h1>

			<div class="formContainer">
				<form id="loginForm">
					<div class="mdc-text-field"  data-mdc-auto-init="MDCTextField">
					  <input type="text" id="username" class="mdc-text-field__input" required>
					  <label class="mdc-floating-label" for="username">Username</label>
					</div>
					<div class="mdc-text-field"  data-mdc-auto-init="MDCTextField">
					  <input type="password" id="password" class="mdc-text-field__input" required>
					  <label class="mdc-floating-label" for="password">Password</label>
					</div>
			        <button type="submit" id="submitBtn" class="mdc-button mdc-button--raised">
			        	<i class="material-icons mdc-button__icon">launch</i>
			        	Login
			    	</button> 
				</form>
			</div>
		</div>
	</div>
	<script src="js/login.js"></script>

{{> footer-end }}